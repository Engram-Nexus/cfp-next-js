    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-toast": "^1.1.5",
    "ai": "^3.0.19",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "drizzle-kit": "^0.20.14",
    "drizzle-orm": "^0.30.8",
    "drizzle-zod": "^0.5.1",
    "hono": "^4.2.4",
    "jose": "^5.2.4",
    "lucide-react": "^0.365.0",
    "next": "14.1.0",
    "openai": "^4.38.2",
